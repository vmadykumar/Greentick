@model ComplyRite.Web.MVC.Areas.ReportMgmt.Models.AuditArea
@using ComplyRite.Web.MVC.Areas.ReportMgmt.App_LocalResources

@******** Compliance level for Training & Records Keeping Graph *******@
<div class="col-lg-12 row">
    <div class="col-lg-5 col-lg-offset-4">
        <canvas id="trainingRecordKeeping-piechart" trainingRecordKeeping-yes-data="@Model.PassedCheckpointPercentage" trainingRecordKeeping-no-data="@Model.FailedCheckpointPercentage" label="@Model.AreaOfAudit" width="400" height="300" class="canvas-custom-css"></canvas>
        <span><img id="trainingRecordKeeping-piechart-img" class="canvasImg-custom-css" /></span>
    </div>
</div>
@**********************************************************************@


@******************* Timestamp Table & Attachments*********************@
    @Html.Partial(@PartialViewsPathInfo.TimestampTableAndAttachments, Model.AuditDetails)
    @Html.Partial(@PartialViewsPathInfo.AttachmentsSection, Model)
@**********************************************************************@

