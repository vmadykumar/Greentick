@model ComplyRite.Web.MVC.Areas.ReportMgmt.Models.AuditArea
@using ComplyRite.Web.MVC.Areas.ReportMgmt.App_LocalResources

@******** Compliance level for Safe Water Handling Practices Graph ******@
<div class="col-lg-12 row">
    <div class="col-lg-5 col-lg-offset-4">
        <canvas id="safeWaterHandling-piechart" safeWaterHandling-yes-data="@Model.PassedCheckpointPercentage" safeWaterHandling-no-data="@Model.FailedCheckpointPercentage" label="@Model.AreaOfAudit" width="400" height="300" class="canvas-custom-css"></canvas>
        <span><img id="safeWaterHandling-piechart-img" class="canvasImg-custom-css" /></span>
    </div>
</div>
@***********************************************************************@

@******************* Timestamp Table & Attachments*********************@

@Html.Partial(@PartialViewsPathInfo.TimestampTableAndAttachments, Model.AuditDetails)
@Html.Partial(@PartialViewsPathInfo.AttachmentsSection, Model)

@**********************************************************************@