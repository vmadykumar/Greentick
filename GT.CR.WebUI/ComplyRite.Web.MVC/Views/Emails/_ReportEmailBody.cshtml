    var auditDate = Model.AuditDate.ToString("dddd dd MMMM yyyy");
}
<div style="font-size: 14px;font-family:Times New Roman,serif; padding-left: 5%; padding-right: 10%;">
    <b>Dear @Model.EmailRecipientName,</b><br /><br />
